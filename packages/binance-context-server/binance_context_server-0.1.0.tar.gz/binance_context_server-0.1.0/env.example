# Binance API Configuration
# Get your API keys from: https://www.binance.com/en/my/settings/api-management

# Binance API Key (optional - only needed for account data)
BINANCE_API_KEY=your_api_key_here

# Binance API Secret (optional - only needed for account data)
BINANCE_API_SECRET=your_api_secret_here

# Use Binance Testnet (true/false)
BINANCE_TESTNET=false

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
