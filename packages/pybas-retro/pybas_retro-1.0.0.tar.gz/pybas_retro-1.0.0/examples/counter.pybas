10 LET COUNTER = 1
30 PRINT "Starting counter at: " + str(COUNTER)
40 LET COUNTER = COUNTER + 1
50 PRINT "Counter incremented to: " + str(COUNTER)
60 LET COUNTER = COUNTER * 2
70 PRINT "Counter doubled to: " + str(COUNTER)
80 LET COUNTER = COUNTER - 3
90 PRINT "Counter minus 3: " + str(COUNTER)
100 PRINT "Final counter value: " + str(COUNTER)
110 END