10 REM Python Set Examples in PyBAS
20 PRINT "=== SET EXAMPLES ==="
30 PRINT ""
40 REM Creating sets
50 LET NUMBERS = {1, 2, 3, 4, 5}
60 PRINT "Number set: " + str(NUMBERS)
70 LET FRUITS = {"apple", "banana", "orange"}
80 PRINT "Fruit set: " + str(FRUITS)
90 PRINT ""
100 REM Set from list (removes duplicates)
110 LET DUPLICATES = [1, 2, 2, 3, 3, 3, 4]
120 LET UNIQUE = set(DUPLICATES)
130 PRINT "Original list with duplicates: " + str(DUPLICATES)
140 PRINT "Set (unique values): " + str(UNIQUE)
150 PRINT ""
160 REM Set operations
170 LET SET_A = {1, 2, 3, 4}
180 LET SET_B = {3, 4, 5, 6}
190 PRINT "Set A: " + str(SET_A)
200 PRINT "Set B: " + str(SET_B)
210 PRINT "Union (A | B): " + str(SET_A | SET_B)
220 PRINT "Intersection (A & B): " + str(SET_A & SET_B)
230 PRINT "Difference (A - B): " + str(SET_A - SET_B)
240 PRINT ""
250 REM Set membership testing
260 LET HAS_APPLE = "apple" in FRUITS
270 IF HAS_APPLE = 1 GOTO 290
280 GOTO 300
290 PRINT "Apple is in the fruit set!"
300 PRINT ""
310 REM Set size and operations
320 PRINT "Number of fruits: " + str(len(FRUITS))
330 LET FRUITS_LIST = list(FRUITS)
340 PRINT "Fruits as list: " + str(FRUITS_LIST)
350 PRINT ""
360 REM Adding to sets (create new set)
370 LET MORE_FRUITS = FRUITS | {"grape", "kiwi"}
380 PRINT "Original fruits: " + str(FRUITS)
390 PRINT "With more fruits: " + str(MORE_FRUITS)
400 PRINT ""
410 REM Empty set
420 LET EMPTY_SET = set()
430 PRINT "Empty set: " + str(EMPTY_SET)
440 PRINT "Empty set size: " + str(len(EMPTY_SET))
450 PRINT ""
460 REM Set from range
470 LET EVEN_NUMS = {2, 4, 6, 8}
480 PRINT "Even numbers: " + str(EVEN_NUMS)
490 END