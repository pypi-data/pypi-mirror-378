{
    "jupyter.lab.shortcuts": [],
    "title": "trainwave-jupyter",
    "description": "trainwave settings.",
    "type": "object",
    "properties": {
        "token": {
            "title": "Authentication Token",
            "description": "Stored authentication token for Trainwave API",
            "type": "object",
            "properties": {
                "access_token": {
                    "type": "string",
                    "description": "Access token for API authentication"
                },
                "refresh_token": {
                    "type": "string",
                    "description": "Refresh token for token renewal"
                },
                "expires_at": {
                    "type": "number",
                    "description": "Token expiration timestamp"
                },
                "token_type": {
                    "type": "string",
                    "description": "Type of token (usually 'Bearer')"
                }
            }
        },
        "auth_config": {
            "title": "Authentication Configuration",
            "description": "Configuration for authentication endpoints",
            "type": "object",
            "properties": {
                "client_id": {
                    "type": "string",
                    "description": "OAuth client ID"
                },
                "redirect_uri": {
                    "type": "string",
                    "description": "OAuth redirect URI"
                },
                "auth_url": {
                    "type": "string",
                    "description": "Authorization URL"
                },
                "token_url": {
                    "type": "string",
                    "description": "Token exchange URL"
                },
                "scope": {
                    "type": "string",
                    "description": "OAuth scope"
                }
            }
        },
        "settings": {
            "title": "User Settings",
            "description": "User preferences for Trainwave extension",
            "type": "object",
            "properties": {
                "organization_id": {
                    "type": "string",
                    "description": "Selected organization UUID for API calls",
                    "default": ""
                },
                "organization_rid": {
                    "type": "string",
                    "description": "Selected organization RID (Resource ID) for display",
                    "default": ""
                },
                "project_id": {
                    "type": "string",
                    "description": "Selected project UUID for API calls",
                    "default": ""
                },
                "project_rid": {
                    "type": "string",
                    "description": "Selected project RID (Resource ID) for display",
                    "default": ""
                },
                "gpu_type": {
                    "type": "string",
                    "description": "Selected GPU type for jobs",
                    "default": "CPU"
                },
                "gpu_count": {
                    "type": "number",
                    "description": "Number of GPUs to use for jobs",
                    "default": 1,
                    "minimum": 1
                }
            }
        }
    },
    "additionalProperties": false
}