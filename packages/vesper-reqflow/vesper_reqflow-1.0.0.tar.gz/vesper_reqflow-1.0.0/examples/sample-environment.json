{
  "environments": {
    "development": {
      "description": "Development environment configuration",
      "variables": {
        "API_BASE_URL": "http://localhost:3000",
        "API_VERSION": "v1",
        "DEBUG": "true",
        "TIMEOUT": "10",
        "GITHUB_TOKEN": "your-dev-github-token",
        "DATABASE_URL": "postgresql://localhost:5432/api_dev",
        "LOG_LEVEL": "DEBUG",
        "CACHE_TTL": "60"
      }
    },
    "staging": {
      "description": "Staging environment configuration",
      "variables": {
        "API_BASE_URL": "https://staging-api.example.com",
        "API_VERSION": "v1",
        "DEBUG": "false",
        "TIMEOUT": "30",
        "GITHUB_TOKEN": "your-staging-github-token",
        "DATABASE_URL": "postgresql://staging-db.example.com:5432/api_staging",
        "LOG_LEVEL": "INFO",
        "CACHE_TTL": "300"
      }
    },
    "production": {
      "description": "Production environment configuration",
      "variables": {
        "API_BASE_URL": "https://api.example.com",
        "API_VERSION": "v2",
        "DEBUG": "false",
        "TIMEOUT": "60",
        "GITHUB_TOKEN": "your-prod-github-token",
        "DATABASE_URL": "postgresql://prod-db.example.com:5432/api_prod",
        "LOG_LEVEL": "WARNING",
        "CACHE_TTL": "3600"
      }
    },
    "testing": {
      "description": "Testing environment configuration",
      "variables": {
        "API_BASE_URL": "https://httpbin.org",
        "API_VERSION": "v1",
        "DEBUG": "true",
        "TIMEOUT": "5",
        "TEST_MODE": "true",
        "MOCK_RESPONSES": "true",
        "LOG_LEVEL": "DEBUG"
      }
    }
  },
  "common_variables": {
    "USER_AGENT": "Agentic-API-Tester/1.0",
    "ACCEPT_HEADER": "application/json",
    "CONTENT_TYPE": "application/json",
    "TIMESTAMP": "2024-01-01T00:00:00Z",
    "REQUEST_ID": "req-${RANDOM_UUID}",
    "CLIENT_VERSION": "1.0.0"
  },
  "sensitive_variables": [
    "GITHUB_TOKEN",
    "API_KEY",
    "SECRET_KEY",
    "PASSWORD",
    "DATABASE_URL",
    "PRIVATE_KEY"
  ]
}