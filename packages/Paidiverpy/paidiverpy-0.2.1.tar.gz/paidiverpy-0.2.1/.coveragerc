[run]
branch = True
source =
    src/paidiverpy
omit =
    src/paidiverpy/utils/benchmark/benchmark_test.py
    src/paidiverpy/utils/schema_json_handler.py
    src/paidiverpy/models/config_model.py
    src/paidiverpy/frontend/*


[report]
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    if __name__ == .__main__.:
    @deprecated

[paths]
source =
    src/paidiverpy
    */site-packages/paidiverpy
