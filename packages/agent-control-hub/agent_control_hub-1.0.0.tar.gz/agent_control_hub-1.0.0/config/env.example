# Agent Control Hub Environment Configuration

# LLM Provider Configuration
# Options: gemini, together, openrouter, local
LLM_PROVIDER=gemini

# API Keys (set the one for your chosen provider)
GOOGLE_API_KEY=your_google_api_key_here
TOGETHER_API_KEY=your_together_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# LLM Model Configuration
LLM_MODEL=gemini-1.5-flash

# API Base URLs (optional, will use defaults if not set)
# LLM_API_BASE=https://api.together.xyz/v1/chat/completions
# LLM_API_BASE=https://openrouter.ai/api/v1/chat/completions
# LLM_API_BASE=http://localhost:11434/v1/chat/completions

# Agent Hub Server Configuration
AGENT_HUB_API_BASE=http://127.0.0.1:8000
PORT=8000
HOST=127.0.0.1

# Logging Configuration
LOG_LEVEL=INFO

# Project Configuration
PROJECTS_DIR=agent_workspace/projects
TEMP_TTL_MINUTES=60
