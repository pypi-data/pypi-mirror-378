# Example VCP CLI configuration file

feature_flags:
  data_command: false
  model_command: false
  benchmarks_command: true

# VCP Models API Configuration
models:
  base_url: "https://models.api.example.com/v1"

# Data API Configuration
data:
  base_url: "https://data.api.example.com/v1"

benchmarks_api:
  base_url: "https://benchmarks.api.example.com/v1"

aws:
  region: "us-west-2"
  cognito:
    user_pool_id: ""
    client_id: ""
    client_secret: ""
    domain: ""

# Databricks configuration for model downloads
# Replace with your actual Databricks workspace host and personal access token
# Example:
# databricks:
#   host: "https://<your-databricks-instance>.cloud.databricks.com"
#   token: "dapiXXXXXXXXXXXXXXXXXXXXXXXX"
databricks:
  host: ""
  token: ""
