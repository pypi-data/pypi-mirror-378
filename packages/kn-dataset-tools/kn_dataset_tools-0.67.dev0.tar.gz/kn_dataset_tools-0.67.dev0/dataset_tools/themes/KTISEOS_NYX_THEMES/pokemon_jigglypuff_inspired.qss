/* POKEMON JIGGLYPUFF INSPIRED THEME */
/* Soft pinks and creams inspired by the adorable singing Pokemon */

QMainWindow {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffb6c1, stop: 0.5 #ffc0cb,
                                stop: 1 #f8bbd9);
    color: #8b0045;
    border: none;
}

QWidget {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffb6c1, stop: 0.5 #ffc0cb,
                                stop: 1 #f8bbd9);
    color: #8b0045;
    font-size: 13px;
    font-weight: bold;
}

/* Sleepy Jigglypuff buttons */
QPushButton {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff69b4, stop: 0.5 #ff1493,
                                stop: 1 #dc143c);
    border: 3px outset #ffffff;
    border-radius: 15px;
    padding: 12px 18px;
    color: #ffffff;
    min-height: 16px;
    font-weight: bold;
}

QPushButton:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff8fbc, stop: 0.5 #ff69b4,
                                stop: 1 #ff1493);
    border: 3px outset #fffafa;
    text-shadow: 1px 1px 2px #8b0045;
}

QPushButton:pressed {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #dc143c, stop: 0.5 #b22222,
                                stop: 1 #8b0045);
    border: 3px inset #ffffff;
}

QPushButton:disabled {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #d3d3d3, stop: 1 #a9a9a9);
    border: 3px outset #c0c0c0;
    color: #808080;
}

/* Balloon text areas */
QTextEdit, QPlainTextEdit {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    border: 4px outset #ff69b4;
    border-radius: 15px;
    padding: 15px;
    color: #8b0045;
    selection-background-color: #ff69b4;
    selection-color: #ffffff;
}

QTextEdit:focus, QPlainTextEdit:focus {
    border: 4px outset #ff1493;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffffff, stop: 1 #fffafa);
}

/* Cute lists */
QListWidget {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    border: 4px outset #ff69b4;
    border-radius: 15px;
    alternate-background-color: #ffeef0;
    color: #8b0045;
    gridline-color: #ffb6c1;
    outline: none;
}

QListWidget::item {
    padding: 12px 15px;
    border-bottom: 2px solid #ffb6c1;
    border-radius: 8px;
    margin: 3px;
}

QListWidget::item:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff69b4, stop: 1 #ff1493);
    color: #ffffff;
    border-radius: 12px;
    border: 2px solid #ffffff;
    font-weight: bold;
}

QListWidget::item:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    color: #8b0045;
    border-radius: 12px;
}

/* Musical menu */
QMenuBar {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    color: #8b0045;
    border-bottom: 4px outset #ff69b4;
    font-weight: bold;
}

QMenuBar::item {
    background: transparent;
    padding: 10px 16px;
    border-radius: 8px;
}

QMenuBar::item:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff69b4, stop: 1 #ff1493);
    color: #ffffff;
    border: 2px outset #ffffff;
}

QMenu {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    border: 4px outset #ff69b4;
    border-radius: 12px;
    padding: 8px;
}

QMenu::item {
    padding: 10px 24px;
    border-radius: 8px;
    margin: 2px;
    color: #8b0045;
}

QMenu::item:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff69b4, stop: 1 #ff1493);
    color: #ffffff;
}

/* Round scrollbars */
QScrollBar:vertical {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    width: 16px;
    border: 3px outset #ffffff;
    border-radius: 8px;
    margin: 0;
}

QScrollBar::handle:vertical {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop: 0 #ff69b4, stop: 0.5 #ff8fbc,
                                stop: 1 #ff1493);
    border: 3px outset #ffffff;
    border-radius: 6px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop: 0 #ff8fbc, stop: 0.5 #ffb6c1,
                                stop: 1 #ff69b4);
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    height: 16px;
    border: 3px outset #ffffff;
    border-radius: 8px;
    margin: 0;
}

QScrollBar::handle:horizontal {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff69b4, stop: 0.5 #ff8fbc,
                                stop: 1 #ff1493);
    border: 3px outset #ffffff;
    border-radius: 6px;
    min-width: 30px;
}

/* Fairy-type tabs */
QTabWidget::pane {
    border: 4px outset #ff69b4;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    border-radius: 12px;
}

QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    border: 4px outset #ffffff;
    padding: 12px 20px;
    margin-right: 4px;
    border-top-left-radius: 12px;
    border-top-right-radius: 12px;
    font-weight: bold;
    color: #8b0045;
}

QTabBar::tab:selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff69b4, stop: 1 #ff1493);
    border: 4px outset #ffffff;
    border-bottom: none;
    color: #ffffff;
}

QTabBar::tab:hover:!selected {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff8fbc, stop: 1 #ffc0cb);
    border: 4px outset #ff69b4;
    color: #8b0045;
}

/* Poke Center status bar */
QStatusBar {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    border-top: 4px outset #ff69b4;
    color: #8b0045;
    font-weight: bold;
}

/* Song input fields */
QLineEdit {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffffff, stop: 1 #fffafa);
    border: 4px outset #ff69b4;
    border-radius: 10px;
    padding: 10px;
    color: #8b0045;
    selection-background-color: #ff69b4;
    selection-color: #ffffff;
}

QLineEdit:focus {
    border: 4px outset #ff1493;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffffff, stop: 1 #fff8f8);
}

/* Pokeball combo boxes */
QComboBox {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffc0cb, stop: 1 #ffb6c1);
    border: 4px outset #ffffff;
    border-radius: 10px;
    padding: 10px;
    color: #8b0045;
    min-width: 90px;
    font-weight: bold;
}

QComboBox:hover {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ff8fbc, stop: 1 #ffc0cb);
    border: 4px outset #ff69b4;
}

QComboBox::down-arrow {
    image: none;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 10px solid #8b0045;
}

QComboBox QAbstractItemView {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    border: 4px outset #ff69b4;
    border-radius: 10px;
    selection-background-color: #ff69b4;
    selection-color: #ffffff;
    color: #8b0045;
}

/* Dream progress bars */
QProgressBar {
    border: 4px outset #ff69b4;
    border-radius: 10px;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    text-align: center;
    color: #8b0045;
    font-weight: bold;
}

QProgressBar::chunk {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop: 0 #ff69b4, stop: 0.5 #ff8fbc,
                                stop: 1 #ffc0cb);
    border-radius: 8px;
    margin: 2px;
}

/* Sleepy tooltips */
QToolTip {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
    border: 4px outset #ff69b4;
    color: #8b0045;
    padding: 10px 15px;
    border-radius: 10px;
    font-weight: bold;
}

/* Additional cute styling */
QLabel {
    color: #8b0045;
    font-weight: bold;
}

QGroupBox {
    border: 4px outset #ff69b4;
    border-radius: 15px;
    margin-top: 15px;
    padding-top: 15px;
    font-weight: bold;
    color: #8b0045;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #fffafa, stop: 1 #f5f5f5);
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 18px;
    padding: 0 15px 0 15px;
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop: 0 #ff69b4, stop: 1 #ff1493);
    color: #ffffff;
    font-weight: bold;
    border-radius: 8px;
    border: 3px outset #ffffff;
}

QCheckBox {
    color: #8b0045;
    font-weight: bold;
}

QCheckBox::indicator {
    width: 18px;
    height: 18px;
    border-radius: 9px;
    border: 3px outset #ff69b4;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #ffffff, stop: 1 #fffafa);
}

QCheckBox::indicator:checked {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1,
                                stop: 0 #ff69b4, stop: 1 #ff1493);
    border: 3px outset #ffffff;
}

QCheckBox::indicator:hover {
    border: 3px outset #ff1493;
}