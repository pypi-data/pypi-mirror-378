[project]
name = "APP_NAME"
version = "0.1.0"
description = "Default template for SplashStand app"
authors = [
    {name = "lesleslie", email = "les@wedgwoodwebworks.com"},
]
dependencies = ["splashstand[analytics,auth,captcha]>=3.0.6", "pydantic[email]>=2.11.3", "fastblocks[admin,monitoring,sitemap]",
"acb[cache,dns,monitoring,requests,secret,smtp,sql,storage]"]
requires-python = "==3.13.*"
readme = "README.md"
license = {text = "BSD-3-CLAUSE"}


[tool.pdm]
distribution = false

[tool.pdm.dev-dependencies]
dev = [
    "-e file:///${PROJECT_ROOT}/../../SplashStand#egg=SplashStand[analytics,auth,captcha]",
    "-e file:///${PROJECT_ROOT}/../../acb#egg=acb[cache,dns,monitoring,requests,secret,smtp,sql,storage]",
    "-e file:///${PROJECT_ROOT}/../../fastblocks#egg=fastblocks[admin,monitoring,sitemap]",
]
