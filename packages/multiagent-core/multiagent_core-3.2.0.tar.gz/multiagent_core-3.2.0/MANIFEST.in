# Include templates directory
recursive-include multiagent_core/templates *

# Include important files
include VERSION
include README.md

# Exclude build artifacts and cache
global-exclude *.pyc
global-exclude __pycache__
global-exclude *.egg-info
# Include template sources
recursive-include .claude *
recursive-include .multiagent *
recursive-include .github *
recursive-include .vscode *
graft .claude
graft .multiagent
graft .github
graft .vscode
