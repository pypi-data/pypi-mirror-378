{
  "pythonVersion": "3.12",
  "pythonPlatform": "All",
  "typeCheckingMode": "standard",
  "reportMissingTypeStubs": false,
  "reportUnusedVariable": false,
  "reportUnusedImport": false,
  "include": [
    "yellhorn_mcp"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "build",
    "dist",
    ".venv",
    "venv",
    "tests",
    "examples",
    "notebooks"
  ],
  "venvPath": ".",
  "venv": ".venv"
}
