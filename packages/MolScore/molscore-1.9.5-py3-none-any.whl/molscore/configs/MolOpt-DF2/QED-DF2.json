{
  "task": "QED",
  "output_dir": "./molscore_results",
  "load_from_previous": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": true,
    "name": "CompoundSimilarity",
    "parameters": {
      "nbmax": 10,
      "minscore": 0.6,
      "minsimilarity": 0.6,
      "radius": 2,
      "useFeatures": false,
      "bits": 2048,
      "outputmode": "linear"
    }
  },
  "scoring_functions": [
    {
      "name": "RDKitDescriptors",
      "run": true,
      "parameters": {
        "prefix": "desc"
      }
    }
  ],
  "scoring": {
    "method": "single",
    "metrics": [
      {
        "name": "desc_QED",
        "weight": 1,
        "modifier": "raw",
        "parameters": {}
      }
    ]
  }
}