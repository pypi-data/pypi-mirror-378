{
  "task": "Troglitazone_rediscovery",
  "output_dir": "./",
  "load_from_previous": false,
  "logging": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": true,
    "name": "CompoundSimilarity",
    "parameters": {
      "nbmax": 10,
      "minscore": 0.6,
      "minsimilarity": 0.6,
      "radius": 2,
      "useFeatures": false,
      "bits": 2048,
      "outputmode": "linear"
    }
  },
  "scoring_functions": [
    {
      "name": "TanimotoSimilarity",
      "run": true,
      "parameters": {
        "prefix": "Troglitazone",
        "ref_smiles": [
          "Cc1c(C)c2OC(C)(CCc2c(C)c1O)COc1ccc(CC2SC(=O)NC2=O)cc1"
        ],
        "radius": 2,
        "bits": null,
        "features": false,
        "counts": true,
        "method": "max",
        "n_jobs": 1
      }
    }
  ],
  "scoring": {
    "method": "single",
    "metrics": [
      {
        "name": "Troglitazone_Sim",
        "weight": 1.0,
        "modifier": "raw",
        "parameters": {}
      }
    ]
  }
}