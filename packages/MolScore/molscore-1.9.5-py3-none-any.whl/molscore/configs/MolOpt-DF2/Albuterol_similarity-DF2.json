{
  "task": "Albuterol_similarity",
  "output_dir": "./",
  "load_from_previous": false,
  "logging": false,
  "monitor_app": false,
  "diversity_filter": {
    "run": true,
    "name": "CompoundSimilarity",
    "parameters": {
      "nbmax": 10,
      "minscore": 0.6,
      "minsimilarity": 0.6,
      "radius": 2,
      "useFeatures": false,
      "bits": 2048,
      "outputmode": "linear"
    }
  },
  "scoring_functions": [
    {
      "name": "TanimotoSimilarity",
      "run": true,
      "parameters": {
        "prefix": "Albuterol",
        "ref_smiles": [
          "CC(C)(C)NCC(O)c1ccc(O)c(CO)c1"
        ],
        "radius": 2,
        "bits": null,
        "features": true,
        "counts": true,
        "method": "max",
        "n_jobs": 1
      }
    }
  ],
  "scoring": {
    "method": "single",
    "metrics": [
      {
        "name": "Albuterol_Sim",
        "weight": 1.0,
        "modifier": "lin_thresh",
        "parameters": {
          "objective": "maximize",
          "upper": 0.75,
          "lower": 0.0,
          "buffer": 0.75
        }
      }
    ]
  }
}